{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@playwright/mcp@latest"]
    },
    "context7": {
      "type": "http",
      "url": "https://context7.anthropic.com/mcp"
    },
    "exa": {
      "type": "http",
      "url": "https://api.exa.ai/mcp",
      "headers": {
        "Authorization": "Bearer ${EXA_API_KEY}"
      }
    }
  }
}
